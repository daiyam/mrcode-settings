{// JOB
   "name": "job"
  ,"separatorRegex": "^\\/\\/[^*]\\S* +JOB(?:( |$))"
  ,"strict": "never"
  ,"nested": [
    {// PROC - PEND
    "name": "proc"
    ,"beginRegex": "^\\/\\/[^*]\\S* +PROC(?: |$)"
    ,"endRegex": "^\\/\\/[^*]\\S* +PEND(?: |$)"
    ,"nested": [
        {// STEP
        "name": "exec"
        ,"separatorRegex": "^\\/\\/[^*]\\S* +EXEC "
        ,"nested": [
        {// Bloc commentaires
        "name": "comment"
        ,"whileRegex": "^\\/\\/\\*"
        ,"kind": "comment"
        }
        ,{// DD INSTREAM DATA without DLM
        "name": "dd-instream1"
        ,"beginRegex": "^\\/\\/[^*]\\S* +DD +\\*"
        ,"whileRegex": "^[^\\/][^\\*\\/]"
        ,"foldEOF": true
        }
        ,{// DD INSTREAM DATA with DLM
        "name": "dd2"
        ,"beginRegex": "^\\/\\/[^*]\\S* +DD +DATA,DLM=(.{2,8})(?: |,|$)"
        ,"endRegex": "^\\1"
        ,"nested": false
        }
        ,{// DD FILE
        "name": "dd-file"
        ,"beginRegex": "^\\/\\/[^*]\\S* +DD +\\S+,(?: |$)"
        ,"endRegex": "^\\/\\/ +\\S+[^,](?: |$)"
        }
        ]
    }
    ]
    }
  ]
  }
]
